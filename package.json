{
  "name": "@ko-yelie/kgl",
  "moduleName": "kgl",
  "version": "0.3.3",
  "description": "WebGL library",
  "main": "dist/index.esm.js",
  "unpkg": "dist/index.iife.min.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "bili",
    "examples": "parcel examples/index.html -d examples-dist",
