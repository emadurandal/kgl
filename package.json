{
  "name": "@ko-yelie/kgl",
  "moduleName": "kgl",
  "version": "0.4.12",
  "description": "Minimal WebGL library",
  "main": "dist/index.es.js",
  "unpkg": "dist/index.iife.min.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "bili",
    "publish": "npm run build && npm publish",
    "dev": "parcel examples/index.html",
