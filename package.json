    "pages": "rm -rf docs && npm run docs"
  },
  "browserslist": {
    "ignoreUnknownVersions": "ios_saf 13.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ko-yelie/kgl.git"
  },
  "keywords": [],
  "author": "Ko.Yelie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ko-yelie/kgl/issues"
  },
  "homepage": "https://github.com/ko-yelie/kgl#readme",
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@parcel/transformer-glsl": "^2.5.0",
    "@parcel/transformer-stylus": "^2.5.0",
    "bili": "^5.0.5",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "glsl-curl-noise": "^0.0.4",
    "glsl-random": "^0.0.5",
    "glsl-y-rotate": "^3.0.0",
    "parcel": "^2.5.0",
    "prettier": "^2.6.2",
    "rollup-plugin-glslify": "^1.2.1",
    "stylus": "^0.57.0"
