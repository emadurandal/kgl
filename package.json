  "license": "MIT",
  "year": 2019,
  "bugs": {
    "url": "https://github.com/ko-yelie/kgl/issues"
  },
  "homepage": "https://github.com/ko-yelie/kgl#readme",
  "devDependencies": {
    "banner-package": "^0.1.0",
    "bili": "^4.8.1",
    "parcel-bundler": "^1.12.4",
    "rollup-plugin-glslify": "^1.1.3"
  },
  "dependencies": {
    "glsl-fast-gaussian-blur": "^1.0.2",
    "glsl-random": "^0.0.5"
  }
